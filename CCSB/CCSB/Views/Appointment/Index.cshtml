@{
    Layout = "~/Views/Shared/_LayoutPages.cshtml";
}
<div class="col">
        @if (User.IsInRole(CCSB.Utility.Helper.Admin))
        {
            <div class="form-group col-4" id="divAdminList">
                <label>Select klant</label>
                @Html.DropDownList("userId", new SelectList(ViewBag.UserList, "Id", "Name"),
               new { @class = "form-control select2" })
            </div>
        }
        <div id="calendar"></div>
        <partial name="_AddEditAppointment" />
</div>
@section Scripts{
    <script src="~/js/script-custom-calendar.js"></script>
}